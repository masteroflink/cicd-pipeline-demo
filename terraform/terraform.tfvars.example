# Example Terraform variables file
# Copy to terraform.tfvars and fill in your values

# Render owner ID should be set via environment variable TF_VAR_owner_id
# or RENDER_OWNER_ID for security (find in Render dashboard URL or API)
# owner_id = "YOUR_RENDER_OWNER_ID"

# Service configuration
service_name = "cicd-pipeline-demo"
region       = "oregon"
environment  = "production"
log_level    = "INFO"

# Database URL (from Supabase - set via TF_VAR_database_url environment variable for security)
# database_url = "postgresql://..."
